{
  "vim.useSystemClipboard": true,
  "go.formatTool": "goimports",
  "window.zoomLevel": 1,
  "sql-formatter.dialect": "pl/sql",
  "go.useLanguageServer": true,
  "workbench.editor.closeEmptyGroups": false,
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreview": false,
  "workbench.sideBar.location": "left",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[go]": {
    "editor.tabSize": 4
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vetur.useWorkspaceDependencies": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.nuxt/**": true,
    "**/.dist/**": true,
    "**/node_modules/*/**": true
  },
  "editor.tabSize": 2,
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [
      ".terraform/**/*",
      "**/.terraform/**/*"
    ]
  },
  "terraform.languageServer": {
    "enabled": true,
    "args": []
  },
  "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "dart.flutterSdkPath": "/Users/tt/flutter",
  "dart.openDevTools": "flutter",
  "vscode-neovim.neovimInitVimPaths.darwin": "/Users/tt/.config/nvim/init.vim",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vscode-neovim.useCtrlKeysForInsertMode": false,
  "vscode-neovim.useCtrlKeysForNormalMode": false,
  "editor.scrollBeyondLastLine": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "json.maxItemsComputed": 50000,
  "editor.formatOnSave": true
}
