{
  "title": "Modify Edge Browser Keybind",
  "rules": [
    {
      "description": "[Edge]⌘ + K To Next Tab/⌘ + J To Previous Tab",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "caps_lock"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.microsoft\\.edgemac"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "caps_lock",
                "left_shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}