{
  "title": "Modify Chrome App Keybind",
  "rules": [
    {
      "description": "[Chrome]⌘ + K To Next Tab",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.google\\.Chrome"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_command",
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "[Chrome]⌘ + J To Previous Tab",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.google\\.Chrome"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_command",
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}