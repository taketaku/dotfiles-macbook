{
  "title": "Modify Chrome App Keybind",
  "rules": [
    {
      "description": "Iterm2 kill pane",
      "manipulators": [
        {
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.googlecode\\.iterm2"
              ]
            }
          ],
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "a",
              "modifiers": [
               "control"
              ],
              "lazy": true
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "6"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}